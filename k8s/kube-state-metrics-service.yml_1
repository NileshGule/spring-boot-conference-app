apiVersion: v1
kind: Service
metadata:
  labels:
    app: kube-state-metrics
    k8s-apps: http
  annotations:
    alpha.monitoring.coreos.com/non-namespaced: "true"
  name: kube-state-metrics
spec:
  ports:
    - name: http-metrics
      port: 8080
      targetPort: metrics
      protocol: TCP
  selector:
    run: conferenceapi
